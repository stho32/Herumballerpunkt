{
  "name": "herumballerpunkt-modular",
  "version": "1.0.0",
  "description": "2D Combat Game with Modular Architecture",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "http-server src -p 8080 -c-1",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "format": "prettier --write src/**/*.js",
    "docs": "jsdoc -c jsdoc.conf.json",
    "analyze": "vite-bundle-analyzer",
    "start": "npm run serve"
  },
  "keywords": [
    "game",
    "2d",
    "javascript",
    "modular",
    "architecture",
    "canvas"
  ],
  "author": "Game Developer",
  "license": "MIT",
  "devDependencies": {
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsdoc": "^46.8.0",
    "http-server": "^14.1.1",
    "jsdoc": "^4.0.2",
    "prettier": "^3.0.3",
    "vite": "^4.4.9",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {},
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stho32/Herumballerpunkt.git"
  },
  "bugs": {
    "url": "https://github.com/stho32/Herumballerpunkt/issues"
  },
  "homepage": "https://github.com/stho32/Herumballerpunkt#readme"
}
